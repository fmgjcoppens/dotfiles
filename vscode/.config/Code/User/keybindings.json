// Place your key bindings in this file to override the defaults
[
    {
        "key": "ctrl+down",
        "command": "cursorMove",
        "when": "editorTextFocus", 
        "args": {
            "to": "nextBlankLine",
            "by": "wrappedLine"
        }
    },
    {
        "key": "ctrl+up",
        "command": "cursorMove",
        "when": "editorTextFocus", 
        "args": {
            "to": "prevBlankLine",
            "by": "wrappedLine"
        }
    }
]